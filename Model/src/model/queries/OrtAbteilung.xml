  <SQLQuery><![CDATA[SELECT departments.department_name AS Abteilung,employees.last_name as Abteilungsleiter, locations.street_address AS Adresse ,locations.city AS Stadt, locations.postal_code AS PLZ,locations.state_province AS Landeskreis, countries.country_name AS Land, regions.region_name AS Region    
FROM locations    
JOIN departments ON locations.location_id = departments.location_id    
JOIN employees ON departments.department_id = employees.department_id   
JOIN countries ON locations.country_id = countries.country_id    
JOIN regions ON countries.region_id = regions.region_id   
WHERE departments.department_id = :DepartmentId1]]></SQLQuery>
